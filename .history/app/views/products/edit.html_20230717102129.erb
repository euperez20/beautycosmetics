<h1>Editing product</h1>

<%= render "form", product: @product %>

<%= form_for @product, html: { multipart: true } do |f| %>
  <!-- Resto del formulario -->
  <%= f.file_field :image %>
  <%= f.submit 'Guardar' %>
<% end %>

<br>

<div>
  <%= link_to "Show this product", @product %> |
  <%= link_to "Back to products", products_path %>
</div>
